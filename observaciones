  `FEC_ALTA` datetime DEFAULT current_timestamp(),
  `ID_USUARIO_BAJA` int(5) DEFAULT NULL,
  `FEC_BAJA` datetime DEFAULT NULL,
  `ID_USUARIO_MODIFICA` int(5) DEFAULT NULL,
