  `REF_UBICACION_ACTA_NACIMIENTO` varchar(100) DEFAULT NULL,
  `IND_INE_BENEFICIARIO` bit(1) DEFAULT NULL,
  `REF_UBICACION_INE_BENEFICIARIO` varchar(100) DEFAULT NULL,
  `REF_DOC_INE_AFILIADO` longtext DEFAULT NULL,
  `REF_DOC_CURP_AFILIADO` longtext DEFAULT NULL,
  `REF_DOC_RFC_AFILIADO` longtext DEFAULT NULL,
  `ID_CONVENIO_PF` int(11) DEFAULT NULL,
