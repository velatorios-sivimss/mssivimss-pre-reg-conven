CREATE TABLE `svt_contratante_beneficiarios` (
  `ID_CONTRATANTE_BENEFICIARIOS` int(11) NOT NULL,
  `ID_CONTRA_PAQ_CONVENIO_PF` int(11) DEFAULT NULL,
  `ID_PARENTESCO` int(11) DEFAULT NULL,
  `ID_PERSONA` int(11) DEFAULT NULL,
  `ID_USUARIO_ALTA` int(5) DEFAULT NULL,
  `FEC_ALTA` datetime DEFAULT current_timestamp(),
  `ID_USUARIO_BAJA` int(5) DEFAULT NULL,
  `FEC_BAJA` datetime DEFAULT NULL,
  `ID_USUARIO_MODIFICA` int(5) DEFAULT NULL,
  `FEC_ACTUALIZACION` datetime DEFAULT NULL,
  `IND_SINIESTROS` bit(1) DEFAULT b'0',
  `IND_ACTIVO` bit(1) DEFAULT NULL,
  `CVE_ACTA` varchar(50) DEFAULT NULL,
  `IND_INE_BENEFICIARIO` bit(1) DEFAULT b'0',
  `REF_UBICACION_INE_BENEFICIARIO` varchar(100) DEFAULT NULL,
  `IND_ACTA_NACIMIENTO` bit(1) DEFAULT b'0',
  `REF_UBICACION_ACTA_NACIMIENTO` varchar(100) DEFAULT NULL,
  `REF_DOC_ACTA_NACIMIENTO_BENEFICIARIO` longtext DEFAULT NULL,
  `REF_DOC_INE_BENEFICIARIO` longtext DEFAULT NULL,
  `REF_DOCUMENTO_BENEFICIARIO` longtext DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1 COLLATE=latin1_swedish_ci;

